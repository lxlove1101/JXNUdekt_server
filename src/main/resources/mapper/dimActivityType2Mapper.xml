<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- dim_activity_type2 -->
<mapper namespace="mysql.dimActivityType2">
    <!-- 结果映射 -->
    <resultMap id="dimActivityType2Map" type="DimActivityType2Entity">
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="tag" property="tag" jdbcType="VARCHAR" />
        <result column="type1Id" property="type1id" jdbcType="BIGINT" />
        <result column="isValid" property="isValid" jdbcType="INTEGER" />
        <result column="createTime" property="createTime" jdbcType="TIMESTAMP" />
        <result column="modifyTime" property="modifyTime" jdbcType="TIMESTAMP" />
    </resultMap>

    <parameterMap id="DimActivityType2" type="com.jxnudekt.server.entity.DimActivityType2Entity"/>
    
    <!-- 表所有字段 -->
    <sql id="allColumns">
        id, name, tag, type1Id, isValid, createTime, modifyTime
    </sql>
    
    <!-- 查询所有数据 -->
    <select id="findDimActivityType2All" resultMap="dimActivityType2Map">
        SELECT
        <include refid="allColumns" />
        FROM dim_activity_type2
    </select>
    
    <!-- 根据条件查询数据 -->
    <select id="findDimActivityType2ByCondition" resultMap="dimActivityType2Map" parameterType="map">
        SELECT
        <include refid="allColumns" />
        FROM dim_activity_type2 WHERE 1 = 1
        <if test="name != null and name != ''">
            AND name LIKE CONCAT('%',#{name},'%')
        </if>
        <if test="tag != null and tag != ''">
            AND tag LIKE CONCAT('%',#{tag},'%')
        </if>
        <if test="type1id != null">
            AND type1Id = #{type1id}
        </if>
        <if test="isValid != null">
            AND isValid = #{isValid}
        </if>
        <if test="createTime != null">
            AND createTime = #{createTime}
        </if>
        <if test="modifyTime != null">
            AND modifyTime = #{modifyTime}
        </if>
    </select>
    
    <!-- 根据主键查询数据 -->
    <select id="findDimActivityType2ByIds" resultMap="dimActivityType2Map" parameterType="list">
        SELECT
        <include refid="allColumns" />
        FROM dim_activity_type2 WHERE id IN
        <foreach collection="list" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>
    
    <!-- 插入数据 -->
    <insert id="insertDimActivityType2" parameterType="map">
        INSERT INTO dim_activity_type2 (<include refid="allColumns" />)
        VALUES (
            #{id},
            #{name},
            #{tag},
            #{type1id},
            #{isValid},
            #{createTime},
            #{modifyTime}
        )
    </insert>
    
    <!-- 批量插入数据 -->
    <insert id="insertDimActivityType2s" parameterType="list">
        INSERT INTO dim_activity_type2 (<include refid="allColumns" />)
        VALUES (
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.name},
                #{item.tag},
                #{item.type1id},
                #{item.isValid},
                #{item.createTime},
                #{item.modifyTime}
            )
        </foreach>
    </insert>
    
    <!-- 修改数据 -->
    <update id="updateDimActivityType2" parameterType="map">
        UPDATE dim_activity_type2 SET
            name = #{name},
            tag = #{tag},
            type1Id = #{type1id},
            isValid = #{isValid},
            createTime = #{createTime},
            modifyTime = #{modifyTime}
        WHERE id = #{id}
    </update>
    
    <!-- 批量修改数据 -->
    <update id="updateDimActivityType2s" parameterType="list">
        <foreach collection="list" index="index" item="item" separator=";">
            UPDATE dim_activity_type2 SET
                name = #{item.name},
                tag = #{item.tag},
                type1Id = #{item.type1id},
                isValid = #{item.isValid},
                createTime = #{item.createTime},
                modifyTime = #{item.modifyTime}
            WHERE id = #{item.id}
        </foreach>
    </update>
    
    <!-- 根据主键删除数据 -->
    <delete id="deleteDimActivityType2ById" parameterType="string">
        DELETE FROM dim_activity_type2 WHERE id = #{id}
    </delete>
    
    <!-- 根据主键批量删除数据 -->
    <delete id="deleteDimActivityType2ByIds" parameterType="list">
        DELETE FROM dim_activity_type2 WHERE id IN
        <foreach collection="list" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>